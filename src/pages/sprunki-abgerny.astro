---
import GameLayout from '../layouts/GameLayout.astro';

const title = "Sprunki Abgerny - Play Sprunki Abgerny Online";
const description = "Sprunki Abgerny isn't just a regular music game, it's a tribute to ancient cultures and an exploration of the endless possibilities of music. Here, you can not only create music, but also feel the heritage and fusion of cultures.";
const keywords = "Sprunki Abgerny";
const canonicalUrl = "https://www.playfiddlebops.com/sprunki-abgerny/";
const gameImage = "/sprunki-abgerny.png";
const gameIframeUrl = "https://turbowarp.org/1101238518/embed";
---

<GameLayout
  title={title}
  description={description}
  keywords={keywords}
  canonicalUrl={canonicalUrl}
  ogImage="https://www.playfiddlebops.com/sprunki-abgerny.png"
  gameImage={gameImage}
  gameIframeUrl={gameIframeUrl}
